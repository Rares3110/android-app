<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1500"
        motion:autoTransition="animateToEnd"
        motion:staggered="0.65"
        motion:motionInterpolator="linear">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/etEmail"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.32999998"
            motion:layout_constraintHorizontal_bias="0.496"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="0dp"
            android:layout_marginBottom="0dp"
            android:alpha="0" />
        <Constraint
            android:id="@+id/etPassword"
            motion:layout_constraintEnd_toEndOf="@+id/etEmail"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="@+id/etEmail"
            android:layout_marginTop="16dp"
            motion:layout_constraintTop_toBottomOf="@+id/etEmail"
            motion:layout_constraintHorizontal_bias="0.0"
            android:alpha="0" />
        <Constraint
            android:id="@+id/btnSubmit"
            motion:layout_constraintEnd_toEndOf="@+id/etPassword"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/etPassword"
            motion:layout_constraintHorizontal_bias="0.49"
            motion:layout_constraintStart_toStartOf="@+id/etPassword"
            android:layout_marginTop="32dp"
            android:alpha="0" />
        <Constraint
            android:id="@+id/googleSignInButton"
            motion:layout_constraintEnd_toEndOf="@+id/btnSubmit"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.120000005"
            motion:layout_constraintTop_toBottomOf="@+id/btnSubmit"
            motion:layout_constraintHorizontal_bias="0.48"
            motion:layout_constraintStart_toStartOf="@+id/btnSubmit"
            android:alpha="0"
            android:layout_marginTop="12dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/etEmail"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.32999998"
            motion:layout_constraintHorizontal_bias="0.496" />
        <Constraint
            android:id="@+id/etPassword"
            motion:layout_constraintEnd_toEndOf="@+id/etEmail"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="@+id/etEmail"
            android:layout_marginTop="12dp"
            motion:layout_constraintTop_toBottomOf="@+id/etEmail"
            motion:layout_constraintHorizontal_bias="0.0" />
        <Constraint
            android:id="@+id/btnSubmit"
            motion:layout_constraintEnd_toEndOf="@+id/etPassword"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/etPassword"
            motion:layout_constraintHorizontal_bias="0.49"
            motion:layout_constraintStart_toStartOf="@+id/etPassword"
            android:layout_marginTop="24dp" />
        <Constraint
            android:id="@+id/googleSignInButton"
            motion:layout_constraintEnd_toEndOf="@+id/btnSubmit"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.120000005"
            motion:layout_constraintTop_toBottomOf="@+id/btnSubmit"
            motion:layout_constraintHorizontal_bias="0.48"
            motion:layout_constraintStart_toStartOf="@+id/btnSubmit" />
    </ConstraintSet>
</MotionScene>